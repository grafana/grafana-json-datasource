{
  "name": "grafana-json-datasource",
  "version": "1.3.6",
  "description": "A data source plugin for loading JSON APIs into Grafana",
  "keywords": [
    "grafana",
    "json"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "build": "grafana-toolkit plugin:build",
    "test": "grafana-toolkit plugin:test",
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch",
    "sign": "grafana-toolkit plugin:sign"
  },
  "homepage": "https://grafana.com/plugins/marcusolsson-json-datasource",
  "bugs": "https://github.com/grafana/grafana-json-datasource/issues",
  "repository": "github:grafana/grafana-json-datasource",
  "author": {
    "name": "Grafana Labs",
    "url": "https://grafana.com"
  },
  "dependencies": {
    "dayjs": "^1.10.4",
    "jsonata": "^1.8.6",
    "jsonpath-plus": "^6.0.1",
    "memory-cache": "^0.2.0",
    "moment": "2.29.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@emotion/core": "10.0.27",
    "@grafana/data": "9.4.3",
    "@grafana/eslint-config": "^5.1.0",
    "@grafana/runtime": "9.4.3",
    "@grafana/toolkit": "9.4.3",
    "@grafana/ui": "9.4.3",
    "@testing-library/jest-dom": "5.4.0",
    "@testing-library/react": "^10.0.2",
    "@types/lodash": "^4.14.168",
    "@types/memory-cache": "^0.2.1",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "cspell": "6.30.2",
    "emotion": "10.0.27",
    "react-virtualized-auto-sizer": "^1.0.4"
  },
  "resolutions": {
    "minimist": "1.2.8",
    "moment": "2.29.4",
    "moment-timezone": "0.5.41"
  },
  "engines": {
    "node": ">=14"
  }
